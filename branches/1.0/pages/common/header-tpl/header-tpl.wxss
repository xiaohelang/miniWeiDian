.header-contens{
  height: 256rpx;
  position: relative;
}
.head-bg{
  width: 100%;
  height: 256rpx;
}
.header-box{
  width: 100%;
  height: 256rpx;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0,0.4)
}
.header-logo{
  display: flex;
  flex-direction: row;
  color: #fff;
  position: absolute;
  bottom: 10rpx;
  left: 16rpx;
}
.shop-logo{
  width: 88rpx;
  height: 88rpx;
  border-radius: 10rpx;
}
.shop-msg{
  display: flex;
  flex-direction: column;
  margin-left: 18rpx;
  font-size: 28rpx;
}
.shop-name{
  padding: 8rpx 0;
}
.certified{
  width: 108rpx;
  height: 28rpx;
  line-height: 28rpx;
  font-size: 20rpx;
  border-radius: 5rpx;
  text-align: center;
  background: rgb(249,108,158)
}