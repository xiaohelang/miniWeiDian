/* pages/KanList/KanList.wxss */
@import "../common/kanjia-tpl/kanjia-tpl.wxss";

page{
  background: #f4f4f4;
}
.header-box{
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 90rpx;
  background: #fff;
  /* padding-top: 15rpx; */
  border-top: 1px solid #E8E8E8
}
.share-tab{
  display: flex;
  width: 560rpx;
  height: 60rpx;
  line-height: 60rpx;
  border:1px solid #f96c9e;
  margin: auto;
  color: #f96c9e;
  border-radius: 8rpx;
  font-size: 30rpx;
  box-sizing: border-box
}
.flex-box{
  flex: 1;
  text-align: center
}
.on{
  background: #F96C9E;
  color: #fff;
}
.list-box{
  display: flex;
  flex-direction: column;
  background: #fff;
  margin-top: 15rpx;
}
.title-box{
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 25rpx;
  font-size: 28rpx;
  color: #ccc;
  border-bottom: 1px solid #e8e8e8;
}
.title-start{
  float: right;
  color: #f96c9e;
}
.group-footer{
  height: 103rpx;
  line-height: 103rpx;
  border-top: 1px solid #e8e8e8;
  font-size: 28rpx;
  color: #8e8e8e;
  padding: 0 25rpx;
}
.sur-time{
  margin-left: 95rpx;
  color: #8e8e8e;
}
.char-detail{
  display: inline-block;
  width: 165rpx;
  height: 65rpx;
  line-height: 67rpx;
  border:1px solid #f96c9e;
  border-radius: 10rpx;
  text-align: center;
  color: #f96c9e;
  float: right;
  margin-left: 25rpx;
  margin-top: 17rpx;
}  

          
        
      